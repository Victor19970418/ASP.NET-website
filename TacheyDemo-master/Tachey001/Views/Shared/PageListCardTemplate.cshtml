@model IPagedList<Tachey001.ViewModel.consoleViewModel>

<div class="row mt-5" id="proposalAttachments">
    @if (Model.Count != 0)
    {
        foreach (var item in Model)
        {
            @Html.Partial("CardTemplate", item)
        }
    }
    else
    {
        <div class="col-12 text-white text-center">
            <h3><i class="fas fa-search fa-6x"></i></h3>
            <h3>找不到你搜尋的課程</h3>
        </div>
    }

</div>
<div class="row mx-auto">
    @Html.PagedListPager(Model, page => Url.Action("All", "Courses", new { page }))
</div>
